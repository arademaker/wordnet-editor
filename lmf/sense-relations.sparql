select ?lf1 ?id1 ?t1 ?p ?lf2 ?id2 ?t2
{
  ?w1 a wn30:Word .
  ?w1 wn30:lexicalForm ?lf1 .
  ?ws1 wn30:word ?w1 .
  ?s1 wn30:containsWordSense ?ws1 ;
      a ?t1 ;
      wn30:synsetId ?id1 ;
      skos:inScheme <http://logics.emap.fgv.br/wn/> .

  ?w2 a wn30:Word .
  ?w2 wn30:lexicalForm ?lf2 .
  ?ws2 wn30:word ?w2 .
  ?s2 wn30:containsWordSense ?ws2 ;
      a ?t2 ;
      wn30:synsetId ?id2 ;
      skos:inScheme <http://logics.emap.fgv.br/wn/> .

  ?ws1 ?p ?ws2 .
}
